  "main": "dist/orion.min.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:nyj000/orion.git"
  },
  "scripts": {
    "build": "rollup -c",
    "build:test": "rollup -c",
    "start": " webpack-dev-server --config webpack.dev.js --mode development",
